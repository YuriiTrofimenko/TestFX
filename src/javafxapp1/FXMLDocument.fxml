<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" styleClass="pane" stylesheets="@style.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.102" fx:controller="javafxapp1.FXMLDocumentController">
    <children>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="15.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="160.0" AnchorPane.topAnchor="0.0">
         <children>
              <Button fx:id="button" onAction="#handleButtonAction" text="Click Me!" />
              <Label fx:id="label" minHeight="16" minWidth="69" />
         </children>
         <padding>
            <Insets left="15.0" top="15.0" />
         </padding>
      </VBox>
    </children>
</AnchorPane>
